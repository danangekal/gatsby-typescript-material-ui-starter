{
  "name": "gatsby-typescript-material-ui-starter",
  "version": "0.1.0",
  "description": "The default stater gatsby starter with typescript + material ui & latest package",
  "author": "Danang Eko Alfianto <github.com/danangekal>",
  "homepage": "https://github.com/danangekal/gatsby-typescript-material-ui-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danangekal/gatsby-typescript-material-ui-starter.git"
  },
  "bugs": {
    "url": "https://github.com/danangekal/gatsby-typescript-material-ui-starter/issues"
  },
  "license": "MIT",
  "keywords": [
    "gatsby",
    "reactjs",
    "typescript",
    "material-ui",
    "eslint",
    "husky",
    "prettier",
    "lint-staged",
    "pwa",
    "seo"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(js|ts|tsx)\" --quiet --fix",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(js|ts|tsx|css|scss|json|md|html)\" --write",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "gatsby": "^3.0.0",
    "gatsby-plugin-gatsby-cloud": "^2.0.0",
    "gatsby-plugin-image": "^1.0.0",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-offline": "^4.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-sharp": "^3.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-sharp": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --ignore-path .gitignore --quiet --fix",
      "prettier --ignore-path .gitignore --write"
    ],
    "*.{css,scss,json,md,html}": [
      "prettier --ignore-path .gitignore --write"
    ]
  }
}
